{
	"name": "LeakIX",
	"version": "0.0.3",
	"manifest_version": 2,
	"description": "The LeakIX extension warns you about potential security issues on the websites you're visiting",
	"icons": {
		"16": "img/16x16.png",
		"48": "img/48x48.png",
		"128": "img/128x128.png"
	},
	"background": {
		"scripts": ["js/background.js"]
	},
	"browser_action": {
		"default_icon": {
			"19": "img/19x19.png",
			"38": "img/38x38.png",
			"16": "img/16x16.png",
			"48": "img/48x48.png",
			"128": "img/128x128.png"
		},
		"default_title": "LeakIX - click for details",
		"default_popup": "html/popup.html"
	},
	"permissions": [
		"tabs",
		"contextMenus",
		"https://leakix.net/"
	]
}
